{
  "name": "HerdFest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "node_modules/.bin/jest"
  },
  "dependencies": {
    "moment": "^2.11.2",
    "react-native": "^0.21.0"
  },
  "devDependencies": {
    "babel-jest": "^9.0.2",
    "eslint": "^2.2.0",
    "eslint-config-strict": "^8.4.0",
    "eslint-plugin-filenames": "^0.2.0",
    "eslint-plugin-react": "^4.2.0",
    "eslint-plugin-react-native": "^1.0.0",
    "install": "^0.5.5",
    "jest-cli": "^0.9.1",
    "npm": "^3.8.0",
    "react-addons-test-utils": "^0.14.7"
  },
  "jest": {
    "testPathDirs": [
      "<rootDir>"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/react-native/jestSupport/preprocessor.js",
    "setupEnvScriptFile": "<rootDir>/node_modules/react-native/jestSupport/env.js",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "promise",
      "source-map",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "globals": {
      "fetch": true
    },
    "verbose": true,
    "collectCoverage": true,
    "testPathIgnorePatterns": ["/node_modules/", "app/__mocks__/"]
  }
}
